<?xml version="1.0" encoding="utf-8" ?>
<plugin id="custom-plugin-open-setting-view" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>CustomOpenSettingView</name>
  <js-module name="CustomOpenSettingView" src="www/CustomOpenSettingView.js">
    <clobbers target="cordova.plugins.CustomOpenSettingView" />
  </js-module>
  <platform name="ios">
    <config-file parent="/*" target="config.xml">
      <feature name="CustomOpenSettingView">
        <param name="ios-package" value="CustomOpenSettingView" />
      </feature>
    </config-file>
    <source-file src="src/ios/CustomOpenSettingView.m" />
  </platform>
</plugin>